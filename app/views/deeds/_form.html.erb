<%= form_for @deed do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br />
    <br />
    <%= f.label :description %>
    <%= f.text_field :description %>
    <br />
    <br />
    <%= f.label :postcode %>
    <%= f.number_field :postcode %>
    <br />
    <br />
    <%= f.label :date %>
    <%= f.text_field :date %>
    <br />
    <br />
    <%= f.label :image %>
    <%= f.url_field :image %>
    <br />
    <br />
    <%= f.label :karma %>
    <%= f.number_field :karma %>
    <br />
    <br />
    <%= f.label :deed_type %>
    <%= f.select :deed_type, ['request', 'offer'] %>
    <br />
    <br />
    <%= f.submit, :class => 'btn' %>
<% end %>
