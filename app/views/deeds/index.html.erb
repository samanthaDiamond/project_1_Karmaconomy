<div class="h1 center user_title">KARMACONOMY</div>

<section class="container clearfix mxn3">
  <div class="">
    <h3 class="h3 center bold deed_heading">ALL DEEDS</h>
  </div>
  <% @deeds.each do |deed| %>
  <div class="col md-col-3 px1 center deed_box">
      <div class="p2 bg-white border rounded deed_links">
        <%= cl_image_tag(deed.image, :width => 200, :height => 200, :crop => :fill, :class => 'feature mb2') %>
        <h1 class="h2 mt0 center deed_link_box"><%= link_to(deed.title, deed_path(deed)) %></h1>
      </div>
  </div>
  <% end %>
  </div>
</section>
